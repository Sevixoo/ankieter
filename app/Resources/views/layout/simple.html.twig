<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block title %}Ankieter{% endblock %}</title>

    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/materialize/css/materialize.min.css') }}"  media="screen,projection"/>

    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>

    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">



    <script type="text/javascript" src="{{ asset('bundles/materialize/js/materialize.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/underscore/underscore.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ankieter_api/AnkieterApi.js') }}"></script>

</head>
<body style="height: 100%;min-height: 100%" >

    {%include ':parts:panel_nav_simple.html.twig' %}

    <div class="col s12" >
        {% block content %}{% endblock %}
        {% block javascripts %}{% endblock %}
        <div style="clear:both;" ></div>
    </div>


    {% block footer %}
        <footer class="page-footer blue darken-2" style="position: absolute; bottom: 0;width: 100%;height: 70px;">
           {%include 'parts/footer_small.html.twig' %}
        </footer>
    {% endblock %}

</body>
</html>
