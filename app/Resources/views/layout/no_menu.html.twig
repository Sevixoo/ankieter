<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block title %}Ankieter{% endblock %}</title>

    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link type="text/css" rel="stylesheet" href="{{ asset('bundles/materialize/css/materialize.min.css') }}"  media="screen,projection"/>

    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>

    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">

    <script type="text/javascript" src="{{ asset('bundles/materialize/js/materialize.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/underscore/underscore.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/jQueryFileUpload/js/vendor/jquery.ui.widget.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/jQueryFileUpload/js/jquery.iframe-transport.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/jQueryFileUpload/js/jquery.fileupload.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/ankieter_api/AnkieterApi.js') }}"></script>


    <style>
        .ui-autocomplete{
            z-index: 9000000;
        }
    </style>

</head>
<body class="grey lighten-3" style="height: 100%;min-height: 100%" >

    {% block nav %}
        {% include ':parts:panel_nav_creator.html.twig' %}
    {% endblock %}

    <div id="progressLoader" class="progress" style="margin: 0px;height: 5px;display: none;" >
        <div class="indeterminate"> </div>
    </div>

    <div class="col s12">
        {% block content %}{% endblock %}
        <div style="clear:both;" ></div>
    </div>

    {% block footer %}
        <!--footer class="page-footer blue darken-2" style="position: absolute; bottom: 0;width: 100%;height: 70px;">
            {%include 'parts/footer_small.html.twig' %}
        </footer-->
    {% endblock %}

    {% block javascripts %}{% endblock %}

    </body>
</html>